local Aiming = loadstring(game:HttpGet("https://pastebin.com/raw/3A9wdSkL"))()
Aiming.ShowFOV = getgenv().ShowFOV
Aiming.TargetPart = getgenv().Parts
Aiming.HitChance = getgenv().Hitchance
